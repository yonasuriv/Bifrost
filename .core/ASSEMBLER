#!/bin/sh

# origin=~/.local/bin/bifrost

# omega=~/.local/share/bifrost # Final Directory Destination of the Package Folder
#. $omega/.core/ASSEMBLER

bridge_build() {
    omega="." # Debug
}

bridge_set() {
    omega=~/.local/share/bifrost # Final Directory Destination of the Package Folder
}

# Assets
. $omega/.assets/CSS
. $omega/.assets/LOGO

# Menu
. $omega/.menu/MAIN
. $omega/.menu/FIND-DCIP
