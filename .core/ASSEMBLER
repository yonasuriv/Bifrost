. $omega/.info/LEGEND
. $omega/.info/MAN

# Core
. $omega/.core/CONTROL
. $omega/.core/DEPENDENCIES
. $omega/.core/TARGET
. $omega/.core/PROJECTS
. $omega/.core/ARGUMENTS
. $omega/.core/UPDATER
